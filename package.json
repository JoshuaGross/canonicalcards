{
  "name": "dashstock",
  "version": "0.0.1",
  "description": "HighCharts(tm) on a Budget",
  "main": "index.js",
  "scripts": {
    "watch": "watchify src/main.ts -v --debug -p [ tsify --debug --noImplicitAny ] -o public/js/main.js & node server.js & node scripts/less-watch-compiler.js public/less public/css",
    "gwatch": "gulp watch & node server.js & node scripts/less-watch-compiler.js public/less public/css",
    "gulpwatch": "gulp watch & node server.js",
    "build": "NODE_ENV=production browserify src/main.ts -p [ tsify --noImplicitAny ] | uglifyjs -c > dist/dashstock.js",
    "test": "tsc --module commonjs --noImplicitAny src/main.ts & ./node_modules/karma/bin/karma start karma.conf.js",
    "clean": "rm src/*.js & rm public/js/main.js & rm dist/dashstock.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:esbullington/dashstock.git"
  },
  "keywords": [
    "stock",
    "charts"
  ],
  "author": "Eric S. Bullington",
  "license": "Proprietary -- All Rights Reserved",
  "bugs": {
    "url": "https://github.com/esbullington/dashstock/issues"
  },
  "homepage": "https://github.com/esbullington/dashstock",
  "devDependencies": {
    "browserify": "^6.3.3",
    "event-stream": "^3.1.7",
    "express": "^4.10.4",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-browserify": "^0.5.0",
    "gulp-plumber": "^0.6.6",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-typescript": "^2.3.0",
    "gulp-util": "^3.0.1",
    "jsdom": "^1.3.1",
    "karma": "^0.12.28",
    "karma-browserify": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.2.3",
    "karma-mocha": "^0.1.9",
    "karma-phantomjs-launcher": "^0.1.4",
    "tsify": "^0.6.3",
    "watchify": "^2.1.1"
  },
  "dependencies": {
    "d3": "^3.4.13",
    "lodash": "^2.4.1"
  }
}
